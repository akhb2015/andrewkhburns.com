{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "create-block/buy-box",
  "version": "0.1.0",
  "title": "Buy Box",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example static block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "attributes": {
    "buyBoxHeading": {
      "type": "string",
      "source": "html",
      "selector": ".wp-block-create-block-buy-box .content h1"
    },
    "buyBoxText": {
      "type": "string",
      "source": "html",
      "selector": ".wp-block-create-block-buy-box .content p"
    },
    "buyBoxTextColor": {
      "type": "string",
      "default": "green"
    }
  },
  "textdomain": "buy-box",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}